package orientacaoaobjeto.aula01.atributosEmetodos.exercicios.estudante;

import java.time.LocalDate;
import java.time.Month;

public class TestaAluno {

	public static void main(String[] args) {
		
		Curso c01 = new Curso();
		
		c01.nome = "Java";
		c01.turno = "noite";
		c01.cargaHoraria = 120;
		c01.comeco = LocalDate.of(2022, Month.DECEMBER, 01);
		c01.fim = c01.comeco.plusMonths(c01.cargaHoraria /16);
		
		System.out.println(c01.fim);

	}

}
